.edit{
    text-align: center;
}
.detail{
    width:60%;
}
.changeManage{float: right;display: inline-block;
    font-size:0.24rem;color:#4c5568;
    position: relative;}
.changeManage i{
    font-family: iconfont;
    margin-left:0.1rem;
    font-size:0.2rem;
}
.manageList{
    line-height:0.4rem;
    font-size:0.24rem;
    display: inline-block;
    position: absolute;
    right: -5px;
    left:-4px;
    bottom: -0.6rem;
    height: 0.8rem;
    background: #fff;
    border: 1px solid #da3748;
    z-index: 1;
}
.manageList span{
    display: inline-block;
    width: 100%;
    height: 50%;
    margin: 0;
    padding:0;
    padding-left: 0.05rem;
}
.blueOn{background: #da3748;color: #fff;}
.zeroChunk{
    background-color: #f7f7f7;
    height:calc(100% - 0.8rem);
}
@font-face {font-family: 'iconfonttwo';
    src: url('fonttwo/icomoon.eot');
    src: url('fonttwo/icomoon.eot?#iefix') format('embedded-opentype'),
    url('fonttwo/icomoon.woff') format('woff'),
    url('fonttwo/icomoon.ttf') format('truetype'),
    url('fonttwo/icomoon.svg#iconfont') format('svg');
}
.iconfonttwo{
    font-family:"iconfonttwo" !important;
    font-size:16px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
@font-face {font-family: 'iconfontthree';
    src: url('fontthree/icomoon.eot');
    src: url('fontthree/icomoon.eot?#iefix') format('embedded-opentype'),
    url('fontthree/icomoon.woff') format('woff'),
    url('fontthree/icomoon.ttf') format('truetype'),
    url('fontthree/icomoon.svg#iconfont') format('svg');
}
.iconfonttwo{
    font-family:"iconfontthree" !important;
    font-size:16px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
.backSee{
    margin-right:0.3rem;
    position: relative;
}
.backSee i{
    font-family: iconfontthree;
    left:-0.45rem;
}
.imgBox i{
    font-family: iconfontthree;
    position:absolute;
    font-size:0.35rem;
    left:50%;
    top:50%;
    transform: translate(-50%,-50%);
}
.imgBox video{
    width:100%;
    height:100%;
    position:absolute;
    top:0;
    left:0;
}

