.fans{
    width:100%;
    height:100%;
}
header{
    height:0.8rem;
    line-height:0.8rem;
    text-align: center;
    font-size:0.34rem;
    color:#535c71;
    background-color: #fff;
}
#app{
    width:100%;
    height:100%;
}
.liveDetail{
    background:url('../img/fansBg.png') no-repeat ;
    background-size:100% 100%;
    background-position: 0 0;
    width:100%;
    height:2.65rem;
    padding:0.4rem 0 0 0.25rem;
    background-color:#eff3f7;
    position: relative;
}
.liveDetail-title{
    float:left;
    margin-left:0.25rem;
}
.liveDetail-title .name{
    text-align: center;
    font-size:0.28rem;
    margin-bottom:0.3rem;
    color:#fff;
}
.liveDetail-title .certification{
    font-size:0.24rem;
    color:#fff;
    margin-bottom:0.3rem;
}
.liveDetail-title .certification span{
    font-size:0.48rem;
    color:#fff;
    font-weight:bold;
    margin-left:0.2rem;
}
.liveDetail-title .accessTimes{
    font-size:0.26rem;
    color:#ffcbd1;
}
.liveDetail-title .accessTimes span{
    color:#dcdbff;
    margin-left:0.2rem;
}
.divingFans{
    height:0.8rem;
    line-height:0.8rem;
    background-color: #f7f7f7;
    color:#535c71;
    font-size:0.24rem;
    padding-left:0.25rem;
}
.accessDetail{
    border-bottom:0.5px solid #e7e9ef;
    height:1rem;
    margin-left:0.25rem;
    margin-right:0.25rem;
    line-height:1rem;
}
.accessDetail .fansDetail{
    font-size:0.28rem;
    color:#535c71;
}
.accessDetail .statistical{
    float:right;
    font-size:0.26rem;
    color:#d13243;
}
.accessDetail .statistical i{
    width:0.12rem;
    height:0.21rem;
    display:inline-block;
    margin-left:0.16rem;
    background:url('../img/fansBtn.png') no-repeat;
    background-size:100% 100%;
}
.accessItem{
    border-bottom:0.5px solid #e7e9ef;
    height:2.6rem;
    margin-left:0.25rem;
    margin-right:0.25rem;
    padding-top:0.35rem;
    padding-bottom:0.35rem;
}
.accessItem .watchId{
    margin-bottom:0.35rem;
    font-size:0.28rem;
    color:#d13243;
}
.accessItem .watchId .watchTime{
    float:right;
    font-size:0.22rem;
    color:#a3acbe;
    font-weight:normal;
}
.accessItem .watchContent{
    font-size:0.28rem;
    color:#535c71;
    margin-bottom:0.35rem;
}
.accessItem .watchTimea{
    font-size:0.28rem;
    color:#535c71;
}
.fanBgTwo{
    width:2.04rem;
    height:1.89rem;
    float:left;
}
.itemContainer{
    height:calc(100% - 6.15rem);
    width:100%;
}
