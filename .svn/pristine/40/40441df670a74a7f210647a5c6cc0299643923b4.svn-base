header{
    height:1.2rem;
    background-color: #fff;
    line-height:1.2rem;
    text-align: center;
    font-size:0.3rem;
    color:#4c5568;
    width:100%;
    position: relative;
}
.back{
    position: absolute;
    /* top: calc(50% - 0.2rem); */
    left: 0.25rem;
    font-family: iconfont;
    font-size:0.4rem;
    color:#a3acbe;
}
.add{
    display: inline-block;
    width: 0.36rem;
    height: 0.36rem;
    background:url('../img/tianjia-.png') no-repeat center center;
    background-size:100% 100%;
    background-position: 0 0;
    vertical-align: middle;
    position: absolute;
    top: calc(50% - 0.2rem);
    right: 0.25rem;
}
section{
    width:100%;
    padding:0 0.25rem;
}
.title,.describe,.startTime,.endTime{
    outline: none;
    border:none;
    display:block;
    width:100%;
    border-bottom:0.8px solid #d0d3db;
    height:1rem;
    font-size:0.28rem;
    color:#b2bccf;
}
.describe{
    height:1.65rem;
    border:none;
}
.describe input{
    outline: none;
    border:none;
    display:block;
    width:100%;
    font-size:0.28rem;
    color:#b2bccf;
    margin-top:0.35rem;
}
.startTime,.endTime{
    line-height:1rem;
}
.timeIcon,.timeIconTwo{
    font-family: iconfont;
    font-size:0.4rem;
    position:absolute;
    right:0.25rem;
    top:0.3rem;
    color:#d13243;
}
.timeIconTwo{
    top:1.25rem;
}
.endTime{
    border:none;
}
.settingPassword{
    width: 100%;
    padding: 0.35rem 0;
}
.settingPassword p{
    font-size:0.24rem;
    color:#4c5568;
}
.settingPassword .nothing{
    margin-left:0.6rem;
    margin-top:0.2rem;
}
.settingPassword .have i{
    background: url('../img/xuanzhong.png') no-repeat;
    width:0.3rem;
    height:0.3rem;
    display: inline-block;
    background-size:100% 100%;
    vertical-align: -4px;
    margin-right:0.23rem;
}
.settingPassword  i{
    width:0.3rem;
    height:0.3rem;
    display: inline-block;
    background: url('../img/weixuanzhong-.png') no-repeat;
    background-size:100% 100%;
    vertical-align: -4px;
    margin-right:0.23rem;
}
.addPoster,.addVideo{
    width:100%;
    padding-top:0.35rem;
    position: relative;
}
.addPoster p,.addVideo p{
    font-size:0.28rem;
    color:#4c5568;
    padding-bottom:0.35rem;
    border-bottom:0.8px solid #d0d3db;
}
.addPoster img{
    display:block;
    margin:0 auto;
    width:100%;
    height:2.5rem;
}
.addPoster .changeImg{
    width:1.5rem;
    height:0.62rem;
    position:absolute;
    right:0;
    bottom:0.4rem;
}
.addPoster .changeImg .chooseImg{
    width:0.62rem;
    height:0.62rem;
    display:inline-block;
    background:url('../img/chooseImg.png') no-repeat;
    background-size:100% 100%;
}
.addPoster .changeImg .deleteImg{
    width:0.64rem;
    height:0.64rem;
    display:inline-block;
    margin-left:0.22rem;
    background:url('../img/deleteImg.png') no-repeat;
    background-size:100% 100%;
}
.addVideo img{
    display:block;
    margin:0 auto;
    width:100%;
    height:2.5rem;
}
.addPromoteProduct{
    width:100%;
    padding-top:0.3rem 0;
}
.addPromoteProduct div{
    width: 100%;
    padding: 0.35rem 0;
    color: #da3748;
    font-size: 0.28rem;
    border-bottom: 0.8px solid #d0d3db ;
}
.addPromoteProduct p{
    font-size:0.28rem;
    color:#a3acbe;
    margin-top:0.35rem;
}
.addPromoteProduct i{
    display: inline-block;
    float:right;
    width: 0.4rem;
    height: 0.18rem;
    /* background: url(../img/smallicon.png) no-repeat center;
    background-size: cover; */
    vertical-align: middle;
    font-family: iconfont;
    font-size:0.4rem;
    color:#4c5568;
}
.saveChange{
    width: 100%;
    height: 1.3rem;
    display: inline-block;
    color: white;
    line-height: 0.8rem;
    text-align: center;
    padding: 0.25rem;
    border-top: 0.8px solid #c7cdd8;
}
.saveChange span{
    background-color: #d13243;
    width: 100%;
    height: 100%;
    display: inline-block;
    border-radius: 0.1rem
}
#password{
    display: inline-block;
    margin: 0;
    padding: 0;
    width: 2.48rem;
    height: 0.6rem;
    margin-left: 1.9rem;
    outline: none;
    border: 1px solid #d0d3db ;
    font-size: 0.3rem;
    color: #666;
    margin-top: 0.4rem;
    text-align: center;
}
.list{width: 100%;margin-top: 0.35rem;}
.list-item{width: 100%;height: 1.8rem;font-size:0.28rem;color:#4c5568;
    border-bottom: 0.8px solid #d0d3db ;}
.imgBox{width: 30%;height: 100%;padding:0.23rem 0;}
.imgBox img{width: 100%;height: 100%;}
.detail{padding-left: 0.7rem;position: relative;margin-top:0.35rem;}
.detail .nameHeight{height: 65%;margin-bottom:0.3rem;}
.normal{position: absolute;display: inline-block;width: 0.31rem;height: 0.31rem;left: 0;top: 38%;
    background: url("../img/delete.png") no-repeat center;
    background-size: cover;}
.check{background: url("../img/delete.png") no-repeat center;background-size: cover;}
