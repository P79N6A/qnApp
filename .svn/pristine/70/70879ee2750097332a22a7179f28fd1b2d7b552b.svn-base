<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>设置</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-title" content="Web App">
    <meta name="format-detection" content="telephone=no">
    <meta name="viewport" content="width=device-width,initial-scale=1.0"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=yes, minimum-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="css/common.css">
    <link rel="stylesheet" href="css/setting.css">
    <script src="js/rem.js"></script>
</head>
<body>
    <header><p style="border-bottom: 0.5px solid #b7bac4;height:1rem;line-height:1.2rem"><span>设置</span><span class="loginOut">注销</span></p><div style="height:0.2rem;background-color:#f7f7f7;"></div></header>
    <div class="host">
        <i class="icon-host">&#xe90d;</i>
        <span style="font-size:0.3rem;color:#535c71;position:relative;top:-8px;margin-right:0.42rem">主播</span>
        <span class="host-name" id="host-name"></span>
    </div>
    <div style="height:0.2rem;background-color:#f7f7f7;"></div>
    <div class="changePassword" id="changePassword">
        <i class="icon-chpd">&#xe902;</i>
        <span style="font-size:0.3rem;color:#535c71;position:relative;top:-4px;">修改密码</span>
        <i class="point">&#xe905;</i>
    </div>
    <div class="liveSetting" id="liveSetting">
        <i class="icon-setting">&#xe909;</i>
        <span style="font-size:0.3rem;color:#535c71;position:relative;top:-4px;">直播设置</span>
        <i class="point">&#xe905;</i>
    </div>
    <div style="border-bottom:none" class="about" id="about">
        <i class="icon-about">&#xe90e;</i>
        <span style="font-size:0.3rem;color:#535c71;position:relative;top:-3px;">关于</span>
        <i class="point">&#xe905;</i>
    </div>
    <div style="height:calc(100% - 6.4rem);background-color:#f7f7f7">

    </div>
    <!-- <div class="cancellation" id="cancellation">注销</div> -->
</body>
<script src="js/jquery-1.8.3.min.js"></script>
<script src="js/common.js"></script>
<script src="js/setting.js"></script>
<script src="js/login.js"></script>
</html>