header{
    width: 100%;
    height: 0.9rem;
    color: #fff;
    background: #514cea;
    text-align: center;
    line-height: 0.9rem;
    overflow: auto;
    position: relative;
}
.back{
    display: inline-block;
    width: 10%;
    height: 60%;
    vertical-align: middle;
    background: url("../img/fanhui.png") no-repeat center;
    padding-left: 10px;
    background-size: contain;
    margin-top: 3%;
    position: absolute;
    left: 0;
    top: 0;
}
section {
    padding: 0 0.25rem;
}
.change{
    width: 100%;
}
.change p{
    width: 100%;

}
.change p input,.change p textarea{
    width: 100%;
}
.change p input{
    height: 1rem;
    border: none;
    outline: none;
    border-bottom: 1px solid #e1e1e1;
    font-size: 0.3rem;
    color: #333;
}
.change p textarea{
    height: 1.3rem;
    border: none;
    outline: none;
   padding: 0.25rem 0;
    font-size: 0.3rem;
    color:#333;
    border-bottom: 1px solid #e1e1e1;
}
.setPass{
    width: 100%;
    height: 2rem;
    border-bottom: 1px solid #e1e1e1;
    padding-top: 0.4rem;
}
.set{
    display: inline-block;
    width: 0.25rem;
    height: 0.25rem;
    margin-right: 0.2rem;
    background: url("../img/weixuanzhong-.png") no-repeat center;
    background-size: cover;
}
.marLeft{
    margin-left: 0.3rem;
}
.setCheck{
    background: url("../img/xuanzhong.png") no-repeat center;
    background-size: cover;
}
.addProduct{
    width: 100%;
    margin: 0.35rem 0;
    color: #514cea;
}
.lookProduct{
    display: inline-block;
    width: 0.4rem;
    height: 0.18rem;
    background: url("../img/xiala.png") no-repeat center;
    background-size: cover;
    margin-top: 0.1rem;
    vertical-align: middle;
}
.clickProduct{
    background: url("../img/up.png") no-repeat center;
    background-size: cover;
}
.list{width: 100%;margin-top: 0.4rem;}
.list-item{width: 100%;height: 1.4rem;padding-left: 5px;
    border-bottom: 1px solid #e1e1e1;margin-bottom: 10px;}
.imgBox{width: 30%;height: 85%;}
.imgBox img{width: 100%;height: 100%;}
.detail{margin-left: 10px;width: 65%;height: 85%;position: relative;}
.detail .nameHeight{height: 65%;}
.normal{position: absolute;display: inline-block;width: 12%;height: 38%;right: -2%;top: 38%;
    background: url("../img/delete.png") no-repeat center;
    background-size: cover;}
.bottom{
    width: 100%;
    text-align: center;
}
.startLive{
    width: 40%;
    position: fixed;
    bottom: 1%;
    left: 28%;
}
.startLive img{
    width: 100%;
    height: 100%;
}
.live{
    position: absolute;
    left: 27%;
    top: 40%;
    color: #fff;
}
#settingPassword{
    display:inline-block;
    margin:0;
    padding:0;
    width:35%;
    height:0.5rem;
    margin-left:0.2rem;
    outline: none;
    border:none;
    border-bottom:1px solid #d8d8d8;
    font-size:0.3rem;
    color:#666;
}



















